Proceso Calcular_teorema_de_pitagoras
	
	Definir C1 Como Real;
	Definir C2 Como Real;
	Definir H2 Como Real;
	Definir h Como Real;
	
	Escribir "Cual es el valor del Cateto 1?";
	Leer C1;
	Escribir "Cual es el valor del Cateto 2?";
	Leer C2;
	
	H2<- (C1^2 + C2^2) ;
	
	Escribir "La Hipotenusa al cuadrado es ", H2;
	
	
	h= raiz(H2);

	
	
	Escribir "La Hipotenusa es ", h;
	
	
FinProceso
